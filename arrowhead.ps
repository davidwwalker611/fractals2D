%!PS-Adobe-2.0 EPSF-2.0
%
/angle 60 def
/linesize 4 def
/N 7 def
/X
{
    1 dict begin
    /maxdepth exch def
    maxdepth 0 gt {
        maxdepth 1 sub Y
                       F
	               Plus
        maxdepth 1 sub X
                       F
	               Plus
        maxdepth 1 sub Y
    } if
    end
} def
/Y
{
    1 dict begin
    /maxdepth exch def
    maxdepth 0 gt {
        maxdepth 1 sub X
                       F
                       Minus
        maxdepth 1 sub Y
                       F
                       Minus
        maxdepth 1 sub X
    } if
    end
} def
/Plus { angle rotate} def
/Minus { angle neg rotate} def
/F { linesize 0 lineto linesize 0 translate} def

50 600 translate
0 0 moveto
N Y 
F stroke
