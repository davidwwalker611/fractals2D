%!PS-Adobe-2.0 EPSF-2.0
%
/angle 90 def
/linesize 4 def
/N 4 def
/X
{
    1 dict begin
    /maxdepth exch def
    maxdepth 0 gt {
        maxdepth 1 sub X
	               F
        maxdepth 1 sub Y
                       F
        maxdepth 1 sub X
	               Plus
                       F
	               Plus
        maxdepth 1 sub Y
                       F
        maxdepth 1 sub X
                       F
        maxdepth 1 sub Y
	               Minus
                       F
	               Minus
        maxdepth 1 sub X
	               F
        maxdepth 1 sub Y
	               F
        maxdepth 1 sub X
    } if
    end
} def
/Y
{
    1 dict begin
    /maxdepth exch def
    maxdepth 0 gt {
        maxdepth 1 sub Y
                       F
        maxdepth 1 sub X
                       F
        maxdepth 1 sub Y
	               Minus
                       F
	               Minus
        maxdepth 1 sub X
                       F
        maxdepth 1 sub Y
                       F
        maxdepth 1 sub X
                       Plus
                       F
                       Plus
        maxdepth 1 sub Y
                       F
        maxdepth 1 sub X
                       F
        maxdepth 1 sub Y
    } if
    end
} def
/Plus { angle rotate} def
/Minus { angle neg rotate} def
/F { linesize 0 lineto linesize 0 translate} def

10 200 translate
0 0 moveto
N X stroke
