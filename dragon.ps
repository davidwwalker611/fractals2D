%!PS-Adobe-2.0 EPSF-2.0
%
/angle 90 def
/linesize 5 def
/N 12 def
/X
{
    1 dict begin
    /maxdepth exch def
    maxdepth 0 gt {
        maxdepth 1 sub X
	               Plus
        maxdepth 1 sub Y
	               F
	               Plus
    } if
    end
} def
/Y
{
    1 dict begin
    /maxdepth exch def
    maxdepth 0 gt {
                       Minus
                       F
        maxdepth 1 sub X
                       Minus
        maxdepth 1 sub Y
    } if
    end
} def
/Plus { angle rotate} def
/Minus { angle neg rotate} def
/F { linesize 0 lineto linesize 0 translate} def

400 300 translate
0 0 moveto
F
N X stroke
